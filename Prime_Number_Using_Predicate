public class Demo2 {

    public static void main(String[] args ) {

        IntPredicate predict = n -> n>1  && IntStream.range(2, (int) Math.sqrt(n)+1).noneMatch(i -> n%i ==0 ) ;

         List<Integer> L = IntStream.rangeClosed(2,100)
                 .filter(predict)
                 .boxed().toList();

           L.forEach(System.out :: println);

    }
}

